    "react-native-screen": "^1.0.1",
    "react-native-screens": "^3.5.0",
    "react-native-uuid": "^2.0.1",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "react-redux": "^7.2.4",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0"
